<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.controlsfx.glyphfont.*?>

<VBox prefWidth="410.0" spacing="8.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="drawingbot.javafx.controllers.FXVersionControl">
<children>
    <TableView fx:id="tableViewVersions" editable="true" layoutX="8.0" layoutY="8.0" prefHeight="349.0" prefWidth="386.0" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="versionThumbColumn" minWidth="7.0" prefWidth="122.0" />
            <TableColumn fx:id="versionRatingColumn" prefWidth="95.0" sortable="false" text="Rating" />
            <TableColumn fx:id="versionNameColumn" prefWidth="130.0" sortable="false" text="Name" />
            <TableColumn fx:id="versionDateColumn" editable="false" minWidth="0.0" prefWidth="130.0" sortable="false" text="Date" />
            <TableColumn fx:id="versionNotesColumn" prefWidth="47.0" sortable="false" text="Notes" visible="false" />
            <TableColumn fx:id="versionFileColumn" editable="false" prefWidth="47.0" sortable="false" text="File" visible="false" />
            <TableColumn fx:id="versionPFMColumn" minWidth="0.0" prefWidth="144.0" sortable="false" text="PFM" visible="false" />
        </columns>
    </TableView>
    <HBox alignment="CENTER" layoutX="12.0" layoutY="351.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" spacing="10.0">
        <children>
            <Button fx:id="buttonAddVersion" mnemonicParsing="false" prefHeight="24.0" prefWidth="70.0">
                <graphic>
                    <Glyph fontFamily="FontAwesome" icon="PLUS" />
                </graphic>
            </Button>
            <Button fx:id="buttonDeleteVersion" mnemonicParsing="false" prefHeight="24.0" prefWidth="70.0">
                <graphic>
                    <Glyph fontFamily="FontAwesome" icon="MINUS" />
                </graphic>
            </Button>
            <Button fx:id="buttonLoadVersion" mnemonicParsing="false" prefHeight="24.0" prefWidth="70.0">
                <graphic>
                    <Glyph fontFamily="FontAwesome" icon="EYE" />
                </graphic>
            </Button>
            <Button fx:id="buttonMoveUpVersion" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" prefHeight="24.0" prefWidth="70.0">
                <graphic>
                    <Glyph fontFamily="FontAwesome" icon="ARROW_UP" />
                </graphic>
            </Button>
            <Button fx:id="buttonMoveDownVersion" mnemonicParsing="false" prefHeight="24.0" prefWidth="70.0">
                <graphic>
                    <Glyph fontFamily="FontAwesome" icon="ARROW_DOWN" />
                </graphic>
            </Button>
            <Button fx:id="buttonClearVersions" layoutX="320.0" layoutY="10.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="70.0">
                <graphic>
                    <Glyph fontFamily="FontAwesome" icon="ROTATE_LEFT" />
                </graphic>
            </Button>
        </children>
    </HBox>
</children>
   <padding>
      <Insets bottom="10.0" left="8.0" right="8.0" top="10.0" />
   </padding>
</VBox>
